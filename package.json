{
  "name": "@md-oss/cftools.js",
  "version": "1.1.0",
  "description": "JavaScript implementation for the CFTools Data API.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean": "npm run clean:dist && npm run clean:modules",
    "dev": "npm run start:dev",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix && npx eslint test/**/*.ts --fix",
    "prebuild": "npm run clean:dist",
    "build": "tsup",
    "build:docs": "rimraf ./docs && typedoc",
    "start": "node dist/index",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build && npm run build:docs",
    "test": "ts-mocha test/**/*.spec.ts",
    "test:dev": "ts-mocha test/**/*.spec.ts -w --watch-files '**/*.ts'",
    "typecheck": "tsc --noEmit",
    "up:minor": "npx npm-check-updates@latest -u --target minor && npm install",
    "up:major": "npx npm-check-updates@latest -u && npm install"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/chai": "^5.2.3",
    "@types/lodash": "^4.17.23",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.6",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "chai": "^6.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "mocha": "^11.7.5",
    "rimraf": "^6.1.2",
    "sinon": "^21.0.1",
    "ts-mocha": "^11.1.0",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.1",
    "type-fest": "^5.3.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mirasaki/cftools.js.git"
  },
  "keywords": [
    "cftools",
    "cftools-sdk",
    "cftools-api",
    "cftools-data-api",
    "cftools-wrapper",
    "api-wrapper"
  ],
  "author": {
    "name": "Mirasaki Development",
    "email": "info@mirasaki.dev",
    "url": "https://mirasaki.dev"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mirasaki/cftools.js/issues"
  },
  "dependencies": {
    "cache-manager": "^7.2.8",
    "keyv": "^5.5.5",
    "lru-cache": "^11.2.4"
  }
}
